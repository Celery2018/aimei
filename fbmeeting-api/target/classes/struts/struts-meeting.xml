<?xml version="1.0" encoding="GBK" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="meeting" extends="structs-web-json" namespace="/meeting">

        <action name="appointMeeting" method="appointMeeting" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getMeetingInfo" method="getMeeting" class="com.bizconf.meeting.MeetingAction"/>
        <action name="deleteMeeting" method="cancelMeeting" class="com.bizconf.meeting.MeetingAction"/>
        <action name="updateMeeting" method="editMeeting" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getUserMeetings" method="getUserMeetings" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getRoomSize" method="getRoomSize" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getInvitationInfo" method="getInvitationInfo" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getAllMeetings" method="getAllMeetings" class="com.bizconf.meeting.MeetingAction"/>
        <action name="getEngName" method="getEngName" class="com.bizconf.meeting.MeetingAction"/>
    </package>
</struts>